{
  "$schema": "https://zed.dev/schema/icon_themes/v0.2.0.json",
  "name": "Phosphor Icons Theme",
  "author": "Theo Luciano",
  "themes": [
    {
      "name": "Phosphor Icons Theme",
      "appearance": "dark",
      "directory_icons": {
        "collapsed": "./icons/folder-closed.svg",
        "expanded": "./icons/folder-open.svg"
      },
      "file_suffixes": {
        "svg": "svg",
        "gitignore": "git",
        "md": "document",
        "jsx": "jsx"
      },
      "file_icons": {
        "audio": { "path": "./icons/audio.svg" },
